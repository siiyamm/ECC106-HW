#include <stdio.h>

#include <stdlib.h>

void multiplication_table(){

		int i,j;

	for(i=1;i<=10;i++){

		printf("The Multiplication table of the number %d\n",i);

		for(j=1;j<=10;j++){

			printf("%d X %d =%d\n",i,j,i*j);

		}

	}

}

int main(int argc, char *argv[]) {

multiplication_table();

	return 0;

}
